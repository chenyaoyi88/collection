<!-- index.wxml -->
<view class="-container">
  <component-tabbar 
    list="{{tabbarList}}" 
    tabIndex="{{tabIndex}}"
    class="tabbar-class" 
    bindtabClickEvent="tabClickEvent"
  >

    <view slot="tab0">
      <view class="tab0-box">
        <view class="tab0-main-box">

          <view class="tab0-item-point">
            <component-item 
              item-icon-type="start"
              item-value-left-top=""
              item-value-left-center="选择发货地点"
              item-value-right=""
            ></component-item>

            <component-item 
              item-icon-type="end"
              item-value-left-top=""
              item-value-left-center="选择收货地点"
              item-value-right=""
            ></component-item>
          </view>

          <view class="tab0-item-other">
            <component-item 
              item-icon-type="icon"
              item-icon="{{iconList.carType}}"
              item-value-left-center="车型"
              item-valur-right="请选择车型"
            ></component-item>
            <component-item 
              item-icon-type="icon"
              item-icon="{{iconList.time}}"
              item-value-left-center="时间"
              item-valur-right="立即叫车"
            ></component-item>
            <component-item 
              item-icon-type="icon"
              item-icon="{{iconList.extra}}"
              item-value-left-center="额外服务"
              item-valur-right="装卸搬运等额外服务"
            ></component-item>
            <component-item
              item-icon-type="icon"
              item-icon="{{iconList.goods}}"
            >
              <view slot="item-r">
                货物信息
              </view>
            </component-item>
          </view>
        </view>
      </view>

    </view>

    <view slot="tab1">1111</view>
    <view slot="tab2">2222</view>
  </component-tabbar>
</view>