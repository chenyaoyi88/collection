page {
  background-color: #f5f5f5;
}

.coupon-list-box {
  padding: 30rpx;
}

.coupon-list-none {
  text-align: center;
  color: #a0a0a0;
  font-size: 28rpx;
}

.coupon-list-none.fixed {
  position: fixed;
  left: 0;
  top: 0;
  z-index: 4;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.list-no-more {
  font-size: 28rpx;
  text-align: center;
  color: #a0a0a0;
  padding: 10rpx 0;
}

.coupon-not-use {
  background-color: #fff;
  font-size: 28rpx;
  padding: 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}